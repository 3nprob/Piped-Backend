{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:recommended"
  ],
  "ignorePresets": [
    ":prHourlyLimit2"
  ],
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "io.activej:"
      ],
      "groupName": "activej"
    },
    {
      "matchPackagePrefixes": [
        "com.github.firemasterk."
      ],
      "groupName": "Personal Forks",
      "enabled": false
    }
  ]
}
